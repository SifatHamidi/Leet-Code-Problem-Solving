class Solution:
    def checkIfPangram(self,sentence):
        i=0 
        count=0
        letter='abcdefghijklmnopqrstuvwxyz'
        while(i<=len(letter)):
            if letter[i] in sentence:
                count=count+1
                if count >= len(letter):
                    return True
            else:
                return False
            i=i+1
ob1 = Solution()
print(ob1.checkIfPangram("thequickbrownfoxjumpsoverthelazydog")) 
